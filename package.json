{
	"name": "n8n-nodes-pangolin",
	"version": "1.0.0",
	"description": "n8n node to connect and use the Pangolin API",
	"license": "MIT",
	"homepage": "https://github.com/LilTrublMakr/n8n-nodes-pangolin",
	"keywords": [
		"n8n-community-node-package",
		"pangolin",
		"api"
	],
	"author": {
		"name": "LTM"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/LilTrublMakr/n8n-nodes-pangolin.git"
	},
	"scripts": {
		"build": "n8n-node build",
		"build:watch": "tsc --watch",
		"dev": "n8n-node dev",
		"lint": "n8n-node lint",
		"lint:fix": "n8n-node lint --fix",
		"release": "n8n-node release",
		"prepublishOnly": "n8n-node prerelease"
	},
	"files": [
		"dist"
	],
	"n8n": {
	  "n8nNodesApiVersion": 1,
	  "nodes": [
	    "dist/nodes/Pangolin/Pangolin.node.js"
	  ],
	  "credentials": [
	    "dist/credentials/PangolinApi.credentials.js"
	  ]
	},
	"devDependencies": {
		"@n8n/node-cli": "*",
		"eslint": "9.32.0",
		"prettier": "3.6.2",
		"release-it": "^19.0.4",
		"typescript": "5.9.2"
	},
	"peerDependencies": {
		"n8n-workflow": "*"
	}
}
